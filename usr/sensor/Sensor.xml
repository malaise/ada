<?xml version="1.0" encoding="Utf-8"?>
<!DOCTYPE Sensor SYSTEM "Sensor.dtd">

<Sensor>
  <Rules>
    <Rule Name="Antibot">echo "${Line}" | /usr/bin/mail -s "Alert antibot" malaise</Rule>
  </Rules>
  <Filters>
    <Filter File="/var/log/syslog" Rule="Antibot" Tail="10">IP in blacklist</Filter>
  </Filters>
</Sensor>

