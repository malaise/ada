<?xml version="1.1"?>
<!-- DTD for Variable definitions -->
<!-- This allows defining several variables -->
<!ATTLIST Variables>
<!ELEMENT Variables (Var)*>

<!-- Each variable must have a name and a type -->
<!--  and is defined by a value -->
<!ATTLIST Var Name CDATA #REQUIRED 
              Type (Str|Int) "Str">
<!ELEMENT Var (#PCDATA)>

