<?xml version="1.0" encoding="UTF-8"?>

<!-- DTD for Byte_To_Unicode -->
<!-- +++++++++++++++++++++++ -->

<!-- All the Codes (from 0x00 to 0xFF) must be defined -->
<!-- In any order -->
<!ELEMENT Map (Code+)>

<!-- Byte can be in decimal or hexadecimal (16#zz# or 0xzz) -->
<!ATTLIST Code Byte CDATA #REQUIRED>
<!-- Code can be in decimal or hexadecimal (16#zz# or 0xzz) -->
<!ELEMENT Code (#PCDATA)>

